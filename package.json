{
  "name": "my-innovation-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chance": "^1.1.4",
    "jest": "24.9.0",
    "node-sass": "^4.13.1",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1",
    "react-tooltip": "^4.1.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-tooltip": "^3.11.0",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^1.19.1",
    "ssh2": "^0.8.8",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "pretty": "prettier --write --tab-width 4 \"src/**/*.js\"",
    "prepushtest": "jest",
    "format": "prettier \"./src/**/*.+(js|jsx|css|less|scss|ts|tsx|md)\" --write",
    "transfer": "npm run build && node publish.js"
  },
  "pre-push": [
    "prepushtest"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-typescript",
        {
          "allowNamespaces": true
        }
      ]
    ]
  }
}
